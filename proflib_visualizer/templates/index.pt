<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <link rel="stylesheet" type="text/css" href="static/css/proflib-visualizer.css">

        <script src="http://d3js.org/d3.v3.min.js"></script>
        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
        <script src="static/js/links.js"></script>
        <script src="static/js/nodes.js"></script>
        <script src="static/js/tree.js"></script>
        <script src="static/js/visualizer.js"></script>
    </head>

    <body>
        <div id="action-type">
            <select class="action-type" name="node">
                <option selected="toggle">toggle</option>
                <option>select</option>
                <option>add</option>
                <option>delete</option>
                <option>edit</option>
                <option>organize</option>
            </select>
        </div>
        <div id="tree-modify">
            <button id="node-add">ADD</button>
            <button id="node-delete">DELETE</button>
            <button id="node-edit">EDIT</button>
            <button id="node-organize-up">ORGANIZE FUNCTION UP</button>
            <button id="node-organize-down">ORGANIZE FUNCTION DOWN</button>
        </div>
        <div id="proflib-visualizer-container"></div>
        <div id="node-description-container"></div>
        <script>
            var visualizer = new Visualizer();
        </script>
    </body>
</html>
